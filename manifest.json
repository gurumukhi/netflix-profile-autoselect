{
  "manifest_version": 2,
  "name": "netflix-profile-autoselect",
  "description": "Auto selects profile on netflix",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.netflix.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/index.html"
  }
}